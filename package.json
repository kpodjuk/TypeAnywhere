{
  "name": "io.cordova.hellocordova",
  "displayName": "HelloCordova",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "runAndroid": "cordova run android --device",
    "buildAndroid": "cordova build android",
    "buildAndroidKeys": "cordova build android --debug -- --keystore=../my-release-key.keystore --storePassword=password --alias=alias_name --password=password --packageType=apk",
    "runBrowser": "cordova run browser",
    "cordovaClear": "cordova clear"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "devDependencies": {
    "cordova-android": "^12.0.1",
    "cordova-browser": "^7.0.0"
  },
  "cordova": {
    "platforms": [
      "browser",
      "android"
    ]
  },
  "dependencies": {
    "pug": "^3.0.2"
  }
}
